{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ESNext",
      "lib": ["ESNext", "DOM", "DOM.Iterable"],
      "jsx": "react-jsx",
      "moduleResolution": "Bundler",
      "types": ["vite/client"],
      "baseUrl": ".",
      "paths": { "@/*": ["src/*"] },
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true
    },
    "include": ["src", "vite.config.ts"]
  }
  