{
  "name": "ordak-v2",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "packageManager": "npm@11",
  "scripts": {
    "dev": "vite --strictPort --open false",
    "build": "vite build",
    "preview": "vite preview --strictPort --open false",
    "test": "vitest",
    "test:single-url": "vitest run src/tests/single-url-architecture.test.tsx --reporter=dot",
    "lint": "echo 'Note: Full ESLint requires typescript-eslint (not installed). Use npm run type-check instead.'",
    "lint:single-url": "eslint --config .eslintrc.single-url.mjs \"src/**/*.{ts,tsx}\"",
    "validate:architecture": "npm run test:single-url && npm run type-check",
    "type-check": "tsc --noEmit",
    "migrate": "supabase migration up",
    "deploy:staging": "echo \"Deploy staging: implement your command here\"",
    "deploy:prod": "echo \"Deploy prod: implement your command here\"",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "release:patch": "npm run pre-release && npm version patch -m \"chore(release): %s\" && npm run post-release",
    "release:minor": "npm run pre-release && npm version minor -m \"chore(release): %s\" && npm run post-release",
    "release:major": "npm run pre-release && npm version major -m \"chore(release): %s\" && npm run post-release",
    "pre-release": "node -e \"console.log('Preparing releaseâ€¦')\" && npm run changelog",
    "post-release": "git push && git push --tags && echo \"Tag pushed. GitHub Release will auto-publish if Actions is enabled.\"",
    "guard:rpc": "bash scripts/guard-rpc.sh",
    "guard:no-direct-writes": "bash scripts/scan-direct-writes.sh",
    "preflight": "bash scripts/preflight-no-dupes.sh",
    "security:validate": "npm run guard:no-direct-writes && npm run type-check",
    "test:e2e": "playwright test --reporter=line",
    "fn:dev:bannos": "supabase functions serve shopify-webhooks-bannos --no-verify-jwt",
    "fn:deploy:bannos": "supabase functions deploy shopify-webhooks-bannos --no-verify-jwt",
    "fn:dev:flourlane": "supabase functions serve shopify-webhooks-flourlane --no-verify-jwt",
    "fn:deploy:flourlane": "supabase functions deploy shopify-webhooks-flourlane --no-verify-jwt",
    "fn:dev:worker": "supabase functions serve queue-worker --no-verify-jwt",
    "fn:deploy:worker": "supabase functions deploy queue-worker --no-verify-jwt",
    "gen:types": "supabase gen types typescript --project-id iwavciibrspfjezujydc > src/types/supabase.ts"
  },
