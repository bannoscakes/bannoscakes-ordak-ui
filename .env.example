# Client app (Vite) - example values only, do NOT commit secrets
VITE_SUPABASE_URL=https://YOUR-SUPABASE-PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_ANON_KEY
VITE_APP_URL=http://localhost:5173
# Persisting sessions keeps users signed in after refresh. Default false for kiosk-style workflow.
VITE_SUPABASE_PERSIST_SESSION=false
# Optional override for the Supabase auth storage key. Defaults to ordak-auth-token.
VITE_SUPABASE_STORAGE_KEY=ordak-auth-token
# Toggle for RPC facade (mocks vs real). Default true for local dev.
VITE_USE_MOCKS=true
